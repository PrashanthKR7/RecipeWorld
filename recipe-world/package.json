{
  "name": "recipe-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "lint-autofix": "eslint --ext .js,.vue --fix",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "npm run lint-check && npm run lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.5",
    "@fortawesome/free-solid-svg-icons": "5.4.0",
    "@fortawesome/vue-fontawesome": "0.1.1",
    "axios": "^0.18.0",
    "buefy": "^0.7.0",
    "bulma": "^0.7.2",
    "date-fns": "^1.29.0",
    "lodash": "^4.17.11",
    "normalize.css": "8.0.0",
    "nprogress": "^0.2.0",
    "vee-validate": "^2.1.2",
    "vue": "^2.5.17",
    "vue-axios": "^2.1.4",
    "vue-meta": "^1.5.5",
    "vue-router": "^3.0.1",
    "vue-star-rating": "^1.6.1",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1",
    "vuex-map-fields": "^1.3.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^3.0.0",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-e2e-cypress": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "@vue/cli-service": "^3.1.3",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
